{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next",
        "runtime": "nodejs18.x"
      }
    },
    {
      "src": "admin-frontend/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next",
        "runtime": "nodejs18.x"
      }
    },
    {
      "src": "node-express-typeorm/src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["node-express-typeorm/src/**/*.ts"],
        "runtime": "nodejs18.x"
      }
    },
    {
      "src": "admin-backend/src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["admin-backend/src/**/*.ts"],
        "runtime": "nodejs18.x"
      }
    }
  ],
  "routes": [
    {
      "src": "/frontend/_next/static/(.*)",
      "dest": "/_next/static/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/admin-frontend/_next/static/(.*)",
      "dest": "/_next/static/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "node-express-typeorm/src/index.ts"
    },
    {
      "src": "/graphql",
      "dest": "admin-backend/src/index.ts"
    },
    {
      "src": "/graphql-api/(.*)",
      "dest": "admin-backend/src/index.ts"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/admin-frontend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1",
      "continue": true
    },
    {
      "src": "/",
      "dest": "/frontend/index",
      "continue": true
    }
  ]
}