{
  "version": 2,
  "builds": [
    {
      "src": "website/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next",
        "runtime": "nodejs20.x"
      }
    },
    {
      "src": "admindashboard/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next",
        "runtime": "nodejs20.x"
      }
    },
    {
      "src": "node-express-typeorm/src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["node-express-typeorm/src/**/*.ts"],
        "runtime": "nodejs20.x"
      }
    },
    {
      "src": "admin-backend/src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["admin-backend/src/**/*.ts"],
        "runtime": "nodejs20.x"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "node-express-typeorm/src/index.ts"
    },
    {
      "src": "/graphql",
      "dest": "admin-backend/src/index.ts"
    },
    {
      "src": "/admin(/?.*)",
      "dest": "admindashboard"
    },
    {
      "src": "/(.*)",
      "dest": "website"
    }
  ]
}